<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.291" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="126.0" prefWidth="450.0">
            <children>
                <ChoiceBox fx:id="role" layoutX="150.0" layoutY="29.0" prefHeight="26.0" prefWidth="150.0" />
                <PasswordField fx:id="password" layoutX="151.0" layoutY="72.0" prefHeight="26.0" prefWidth="150.0" />
                <Label layoutX="16.0" layoutY="33.0" text="Выберите роль" />
                <Label layoutX="14.0" layoutY="76.0" text="Введите пароль" />
                <Button fx:id="loginAction" layoutX="327.0" layoutY="72.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="26.0" prefWidth="110.0" text="Войти" />
            </children>
        </AnchorPane>
    </children>
</GridPane>
